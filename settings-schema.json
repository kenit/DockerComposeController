{
    "composeCmd": {
        "type": "entry",
        "default": "docker-compose",
        "description": "docker-compose cmd:"
    },
    "composeProjectName": {
        "type": "entry",
        "default": "some_project",
        "description": "the name of project:"
    },
    "composeFilePath": {
        "type": "filechooser",
        "default": "",
        "description": "the docker-compose.yaml used in project:",
        "select-dir" : false
    },
    "refreshInteval": {
        "type": "scale",
        "default": 5,
        "min": 1,
        "max": 30,
        "step": 1,
        "description": "Refresh interval (min):"
    }
}
